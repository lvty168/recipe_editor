<Application x:Class="NewRecipeViewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:NewRecipeViewer"
             xmlns:vm ="clr-namespace:NewRecipeViewer.ViewModels"
             StartupUri="Views/MainWindow.xaml" >
    <Application.Resources>
        <ResourceDictionary>
            <vm:LocatorViewModle x:Key="Locator"/>
            <local:GreenLampConvert x:Key="GreenLampConvert"/>
            <local:FloatToTimeString x:Key="FloatToTimeString"/>
            <local:FloatToScientific x:Key="FloatToScientific"/>
            <local:StepTypeConvert x:Key="StepTypeConvert"/>
            <local:FloatToValveStatus x:Key="FloatToValveStatus"/>
            <local:FloatToSwitchStatus x:Key="FloatToSwitchStatus"/>
            <local:StringTrim x:Key="StringTrim"/>
            <!--<local:TextBoxEnterKeyUpdateBehavior x:Key="TextBoxEnterKeyUpdateBehavior"/>-->
        </ResourceDictionary>
    </Application.Resources>
</Application>
